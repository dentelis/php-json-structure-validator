{
  "name": "dentelis/php-json-structure-validator",
  "description": "Json data structure validation framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Dim Entelis",
      "email": "dentelis@gmail.com",
      "homepage": "https://www.linkedin.com/in/dentelis/",
      "role": "Developer"
    }
  ],
  "keywords": [
    "json",
    "validator",
    "validation",
    "assert",
    "assertion",
    "testing",
    "structure"
  ],
  "support": {
    "issues": "https://github.com/dentelis/php-json-structure-validator/issues"
  },
  "type": "library",
  "require": {
    "php": "~8.2",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "Dentelis\\StructureValidator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "test": "vendor/bin/phpunit tests --fail-on-risky  --display-warnings --display-errors --display-notices  --display-skipped --display-deprecations",
    "psalm": "vendor/bin/psalm --shepherd --show-info=true --no-diff"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.2@dev",
    "vimeo/psalm": "^6.0@dev"
  }
}
